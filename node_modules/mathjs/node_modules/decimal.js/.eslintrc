{
    "env": {
        "amd": true,
        "browser": true,
        "node": true
    },
    "rules": {
        // Override eslint.json defaults
        "no-cond-assign": 0,
        "no-empty-label": 1,
        "no-extra-semi": 0,
        "no-labels": 0,
        "no-sequences": 0,
        "no-shadow": 1,
        "no-unused-expressions": 0,
        "no-use-before-define": 0,
        "curly": [0, "all"],
        "eqeqeq": 0,
        "new-cap": 1
    }
}
